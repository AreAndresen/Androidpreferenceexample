<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout5"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#03A0F4"
    android:orientation="horizontal">


    <TextView
        android:id="@+id/statistikkV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/slackey"
        android:shadowColor="#C52525"
        android:shadowDx="13"

        android:shadowDy="13"
        android:shadowRadius="13"
        android:text="@string/valgStatistikk"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#FCFCFC"
        android:textSize="30sp"
        android:textStyle="bold"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.513"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.044" />

    <Button
        android:id="@+id/slettStatistikk"
        android:layout_width="215dp"
        android:layout_height="79dp"
        android:background="#03E0F4"


        android:fontFamily="@font/slackey"
        android:shadowColor="#004496"
        android:shadowDx="9"
        android:shadowDy="9"
        android:shadowRadius="9"
        android:text="@string/slettStatistikken"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="bold"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.062"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.373" />

    <Button
        android:id="@+id/tilbake"
        android:layout_width="215dp"

        android:layout_height="79dp"
        android:layout_marginTop="40dp"
        android:background="#03E0F4"


        android:fontFamily="@font/slackey"
        android:shadowColor="#004496"
        android:shadowDx="9"
        android:shadowDy="9"
        android:shadowRadius="9"
        android:text="@string/tilbake"
        android:textColor="#FFFFFF"
        android:textSize="24sp"

        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.062"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/slettStatistikk"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/antallRiktige"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="312dp"
        android:layout_marginEnd="71dp"
        android:fontFamily="@font/slackey"
        android:shadowColor="#004496"
        android:shadowDx="9"

        android:shadowDy="9"
        android:shadowRadius="9"

        android:text="@string/antallRiktig"

        android:textAlignment="center"

        android:textColor="#FFFFFF"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/totalAntRiktig"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.384" />

    <TextView
        android:id="@+id/antallFeil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="312dp"
        android:layout_marginTop="39dp"
        android:layout_marginEnd="102dp"
        android:layout_marginBottom="156dp"
        android:fontFamily="@font/slackey"

        android:shadowColor="#C52525"

        android:shadowDx="9"

        android:shadowDy="9"

        android:shadowRadius="9"
        android:text="@string/antallFeil"

        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/totalAntFeil"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/antallRiktige"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/totalAntRiktig"


        android:layout_width="123dp"

        android:layout_height="31dp"
        android:layout_marginTop="136dp"
        android:fontFamily="@font/slackey"
        android:shadowColor="#004496"
        android:shadowDx="9"
        android:shadowDy="9"
        android:shadowRadius="9"
        android:text="0"

        android:textColor="#FFFFFF"

        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.904"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.032" />

    <TextView
        android:id="@+id/totalAntFeil"
        android:layout_width="123dp"

        android:layout_height="31dp"

        android:fontFamily="@font/slackey"
        android:shadowColor="#C52525"
        android:shadowDx="9"
        android:shadowDy="9"
        android:shadowRadius="9"
        android:text="0"
        android:textColor="#FFFFFF"

        android:textSize="22sp"

        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.904"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.576" />

</androidx.constraintlayout.widget.ConstraintLayout>